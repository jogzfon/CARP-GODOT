stac 100	                                           ;total=0
stac 101	                                            ;i=0
                                                          ;Loop:
ldac 101	                                            ;i++
inac		
stac 101

mvac		                                                ;i to R
ldac 100	                                          ;total to AC
add		                                                  ;total+i
stac 100                                         	;total+=i

ldac 102                                         	;n to AC
sub		                                            ;n-i
jpnz 7		                                        ;if n-i!=0 (i.e. i!=n) goto Loop
		                                                ;else continue here
end                                                  ;added to book's code."
